{
  "bootstrap-sha": "ade97a80edd1f7435a53003fa6b0f356d3938352",
  "exclude-paths": [
    "README.md",
    "*.json",
    ".github",
    "doc",
    "docs",
    "output",
    "release-please",
    "openapi-generator/templates/cli",
    "openapi-generator/templates/go",
    "openapi-generator/templates/python",
    "openapi-generator/templates/php",
    "openapi-generator/templates/ruby-client",
    "openapi-generator/templates/typescript-fetch",
    "clients/cli",
    "clients/go",
    "clients/python",
    "clients/php",
    "clients/ruby",
    "clients/typescript"
  ],
  "include-component-in-tag": true,
  "packages": {
    ".": {
      "component": "java",
      "release-type": "simple",
      "version-file": "openapi-generator/templates/java/version.mustache",
      "changelog-path": "clients/java/CHANGELOG.md"
    }
  },
  "$schema": "https://raw.githubusercontent.com/googleapis/release-please/main/schemas/config.json"
}