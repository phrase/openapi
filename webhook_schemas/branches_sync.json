{
  "$schema": "http://json-schema.org/draft-04/schema#",
  "type": "object",
  "properties": {
    "event": {
      "type": "string"
    },
    "message": {
      "type": "string"
    },
    "sent_at": {
      "type": "string"
    },
    "user": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "username": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "gravatar_uid": {
          "type": "string"
        }
      },
      "required": [
        "id",
        "username",
        "name",
        "gravatar_uid"
      ]
    },
    "project": {
      "type": "object",
      "properties": {
        "id": {
          "type": "string"
        },
        "name": {
          "type": "string"
        },
        "slug": {
          "type": "string"
        },
        "main_format": {
          "type": "string"
        },
        "project_image_url": {
          "type": "null"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        },
        "point_of_contact": {
          "type": "null"
        }
      },
      "required": [
        "id",
        "name",
        "slug",
        "main_format",
        "project_image_url",
        "created_at",
        "updated_at",
        "point_of_contact"
      ]
    },
    "branch": {
      "type": "object",
      "properties": {
        "name": {
          "type": "string"
        },
        "base_project_id": {
          "type": "string"
        },
        "branch_project_id": {
          "type": "string"
        },
        "created_at": {
          "type": "string"
        },
        "updated_at": {
          "type": "string"
        },
        "merged_at": {
          "type": "null"
        },
        "state": {
          "type": "string"
        },
        "child_branches": {
          "type": "array",
          "items": [
            {
              "type": "string"
            }
          ]
        },
        "created_by": {
          "type": "object",
          "properties": {
            "id": {
              "type": "string"
            },
            "username": {
              "type": "string"
            },
            "name": {
              "type": "string"
            },
            "gravatar_uid": {
              "type": "string"
            }
          },
          "required": [
            "id",
            "username",
            "name",
            "gravatar_uid"
          ]
        },
        "merged_by": {
          "type": "null"
        }
      },
      "required": [
        "name",
        "base_project_id",
        "branch_project_id",
        "created_at",
        "updated_at",
        "merged_at",
        "state",
        "child_branches",
        "created_by",
        "merged_by"
      ]
    }
  },
  "required": [
    "event",
    "message",
    "sent_at",
    "user",
    "project",
    "branch"
  ]
}